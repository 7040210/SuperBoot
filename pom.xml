	<groupId>org.superboot</groupId>
	<artifactId>super-boot</artifactId>
	<name>super-boot</name>
	<version>1.0-SNAPSHOT</version>
	<description>SuperBoot</description>
	<packaging>pom</packaging>

	<modules>
		<module>super-boot-utils</module>
		<module>super-boot-base</module>
        <module>super-boot-global</module>
        <module>super-boot-registry</module>
        <module>super-boot-config</module>
        <module>super-boot-gateway</module>
